{
  "author": "Dante Barboza",
  "name": "@dantecoder/react-resizablebox",
  "version": "2.0.0",
  "description": "A React resizable, draggable, rotatable box",
  "repository": "github:DanteCoder/react-resizableBox",
  "keywords": [
    "react",
    "widget",
    "resizable",
    "draggable",
    "rotatable",
    "component"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf dist/ && prettier --write src/ && npm run build:esm && npm run build:cjs && npm run copy-assets",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "copy-assets": "copyfiles -u 1 src/**/*.css dist/cjs/ && copyfiles -u 1 src/**/*.css dist/esm/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@storybook/addon-actions": "^8.0.9",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-interactions": "^8.0.9",
    "@storybook/addon-links": "^8.0.9",
    "@storybook/addon-onboarding": "^8.0.9",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.2",
    "@storybook/blocks": "^8.0.9",
    "@storybook/react": "^8.0.9",
    "@storybook/react-webpack5": "^8.0.9",
    "@storybook/test": "^8.0.9",
    "@types/node": "^18.6.5",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "babel-loader": "^8.2.5",
    "copyfiles": "^2.4.1",
    "file-loader": "^6.2.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "storybook": "^8.0.9",
    "typescript": "^4.9.5",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "peerDependencies": {
    "classnames": "^2.3.1",
    "react": ">17.0.2",
    "react-dom": ">17.0.2"
  },
  "dependencies": {
    "classnames": "^2.3.1"
  }
}
